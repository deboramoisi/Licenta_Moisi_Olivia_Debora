@model IEnumerable<Licenta.Models.Document>

@{
    ViewData["Title"] = "Documente primite";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />

<div class="row">
    <div class="col-12">
        <h2>Documente primite de la utilizatori</h2>
    </div>
</div>
<br />

<div class="table-responsive-md p-4 rounded border">
    <table class="table table-hover" id="tblData">
        <thead>
            <tr class="thead-dark">
                <th>
                    @Html.DisplayNameFor(model => model.DocumentPath)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Client)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ApplicationUserId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TipDocument)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Data)
                </th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/documente/documenteUserPrimite.js"></script>
}