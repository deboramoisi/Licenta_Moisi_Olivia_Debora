@model IEnumerable<ApplicationUser>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@foreach (var user in Model)
{
    <div>
        <a asp-controller="Chat" asp-action="CreatePrivateRoom" asp-route-userId="@user.Id" class="btn btn-secondary">
            @user.UserName
        </a>
    </div>
}